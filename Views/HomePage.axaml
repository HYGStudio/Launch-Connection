<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" Tag="主页"
			 xmlns:ui="using:FluentAvalonia.UI.Controls"
             x:Class="Launch_Connection.Views.HomePage">

	<ScrollViewer>
		<Grid>
			<StackPanel>
				<TextBlock FontWeight="Bold" FontSize="20" Margin="10,10" />
				<Grid x:Name="Notice" />
			</StackPanel>
			<StackPanel x:Name="Notice_Error" HorizontalAlignment="Center" VerticalAlignment="Center">
				<TextBlock Text="公告加载失败"/>
				<Button Margin="0,10,0,0" HorizontalAlignment="Center" Content="重试" />
			</StackPanel>
		</Grid>
	</ScrollViewer>
</UserControl>
